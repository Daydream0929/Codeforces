#include<iostream>
using namespace std;

void cbr(long long  int n,long long int m, long long  int x)
{
	long long int r = x % n - 1;
	if (r == -1)
		r = n -1 ;
	long long int c = (x - (r+1)) / n;
	long long int res = r * m + c + 1;
	cout << res << endl;

}
int main()
{
	int t;
	cin >> t;
	for (int i = 0; i < t; i++)
	{
		long long int n, m, x;
		cin >> n >> m >> x;
		cbr(n, m, x);
	}



	return 0;
}
